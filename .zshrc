# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
#
#
source ~/.zshrc-old

alias pullstsh="git stash && git pull && git stash pop"
alias wake="adb shell input keyevent KEYCODE_WAKEUP"

export ANDROID_HOME=/Users/adrienmorel/Library/Android/sdk

 function duf {
      du -k "$@" | sort -n | while read size fname; do for unit in k M G T P E Z Y; do if [ $size -lt 1024 ]; then echo -e "${size}${unit}t${fname}"; break; fi; size=$((size/1024)); done; done
    }

alias lol='adb reverse tcp:8081 tcp:8081'
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

alias dockerstoplast='docker stop $(docker ps -lq)'
alias yaqua='ssh topbattle@yaqua -p 1614'

alias codp='code-push'

function dockersar {
  docker stop $1 && docker rm $1 && docker rmi $2
}
