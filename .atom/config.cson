"*":
  core:
    disabledPackages: [
      "vim-mode"
      "activate-power-mode"
    ]
    telemetryConsent: "limited"
  editor:
    fontSize: 15
  "exception-reporting":
    userId: "dd0c5768-3b80-a90c-fb86-e615e038e75c"
  linter:
    errorPanelHeight: 145
  "linter-eslint":
    eslintrcPath: "~/.eslintrc.js"
  "ocaml-merlin":
    default: {}
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
