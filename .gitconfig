# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Adrien Morel
	email = hello@adrienmorel.co

[core]
	editor = vim
[push]
	default = simple
[alias]
	st = status
	co = checkout
	ci = commit
  db = push origin --delete
  br = branch
  sth = stash
  cp = cherry-pick
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[credential]
	helper = osxkeychain
